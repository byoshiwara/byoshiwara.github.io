python3 ~/mathbook/pretext/pretext -c webwork -s https://webwork-ptx.aimath.org -d ~/mfg/wwscratch ~/mfg/ptx/mfg.xml 
xsltproc --xinclude -stringparam publisher ~/mfg/pub-html.xml ~/mfg/xsl/mfg-html.xsl ~/mfg/ptx/mfg.xml

python3 ~/mathbook/pretext/pretext -c webwork -d wwscratch -s https://webwork-ptx.aimath.org ../../mfg/ptx/mfg.xml
xsltproc --xinclude --stringparam webwork.extraction ~/byoshiwara.github.io/MFG-dev/wwscratch/webwork-extraction.xml ~/mathbook/xsl/pretext-merge.xsl ~/mfg/ptx/mfg.xml > merge.xml
xsltproc --xinclude ../../mfg/xsl/mfg-html.xsl ./merge.xml

